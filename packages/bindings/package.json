{
  "name": "@serialport/bindings",
  "version": "10.0.0",
  "main": "lib",
  "keywords": [
    "serialport-binding"
  ],
  "dependencies": {
    "@serialport/binding-abstract": "10.0.0",
    "@serialport/parser-readline": "10.0.0",
    "debug": "^4.3.2",
    "node-addon-api": "4.2.0",
    "node-gyp-build": "^4.3.0"
  },
  "devDependencies": {
    "@serialport/binding-mock": "10.0.0",
    "node-abi": "3.5.0",
    "node-gyp": "^7.1.2",
    "prebuildify": "^5.0.0",
    "prebuildify-ci": "^1.0.5",
    "prebuildify-cross": "^4.0.1"
  },
  "engines": {
    "node": ">=12.17.0 <13.0, >=14.0.0"
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "cc --verbose",
    "prebuild": "prebuildify --napi --target 14.0.0 --force --strip --verbose",
    "prebuild-cross": "prebuildify-cross --napi --target 14.0.0 --force --strip --verbose",
    "rebuild": "node-gyp rebuild",
    "prepublishOnly": "prebuildify-ci download"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/serialport/node-serialport.git"
  },
  "gypfile": true,
  "cc": {
    "filter": [
      "legal/copyright",
      "build/include"
    ],
    "files": [
      "src/*.cpp",
      "src/*.h"
    ],
    "linelength": "120"
  },
  "funding": "https://opencollective.com/serialport/donate",
  "binary": {
    "napi_versions": [
      6
    ]
  }
}
